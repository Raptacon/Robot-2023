{
    "files.exclude": {
        "**/.venv": true,
        "**/venv": true
      },
    "flake8.ignorePatterns": [
        "*/tests/pyfrc*",
        "utils/yaml/*",
        ".venv*/",
        "venv*/"
    ],
    "flake8.args": [
        "--select=E9,F6,F7,F8,F4,W1,W2,W4,W5,W6,E11",
        "--ignore=W293,W503"
    ],
    "flake8.importStrategy": "fromEnvironment"
}